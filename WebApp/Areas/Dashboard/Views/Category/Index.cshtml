@model IEnumerable<Category>
<a href="/dashboard/category/create">Create</a>
<table class="table table-bordered">
    <thead>
        <tr>
            <th>id</th>
            <th>name</th>
                   
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>       
                <td>
                    <a href="/dashboard/category/edit/@item.Id">Edit</a>
                </td>
            </tr>
        }
    </tbody>
</table>
